<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:padding="10dip"
	android:layout_width="fill_parent" android:layout_height="fill_parent">
	<TextView android:text="@string/project_name"
		android:layout_width="wrap_content" android:layout_height="wrap_content"
		android:textAppearance="?android:attr/textAppearanceSmall"></TextView>
	<EditText android:layout_marginTop="5dip" android:id="@+id/editTextProjectName"
		android:hint="@string/input_project_name" android:layout_width="fill_parent"
		android:layout_height="wrap_content">
		<requestFocus></requestFocus>
	</EditText>

</LinearLayout>
